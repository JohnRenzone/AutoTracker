        </td>
        <td class="centertd">
          <%= pdf_checkbox(_f, :fuel_filter_serviced) %>
        </td>
        <td>&nbsp;</td>
        <td>
          <%= pdf_grey_checkbox(_f, :spark_plugs) %>
          <p>Spark Plugs</p>
        </td>
        <td class="centertd">
          <%= pdf_checkbox(_f, :spark_plugs_serviced) %>
        </td>
      </tr>
      <tr>
        <td colspan="4">
          <input type="checkbox" class="css-checkboxGrey" name="A82" id="A82"/><label for="A82" class="css-labelGrey"></label>
