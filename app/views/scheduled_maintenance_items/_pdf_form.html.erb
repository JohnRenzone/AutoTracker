        </td>
        <td class="centertd">
          <%= pdf_checkbox(_f, :transmission_fluid_filter_serviced) %>
        </td>
      </tr>
      <tr>
        <td>
          <%= pdf_grey_checkbox(_f, :multipoint_inspection) %>
