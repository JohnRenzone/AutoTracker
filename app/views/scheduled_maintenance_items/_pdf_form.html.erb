        <td class="subheadcell">&nbsp;Due</td>
        <td class="subheadcell">&nbsp;Serviced</td>
        <td>&nbsp;</td>
        <td class="subheadcell">&nbsp;Due</td>
        <td class="subheadcell">&nbsp;Serviced</td>
      </tr>
      <tr>
        <td>
        <td class="centertd">
        </td>
        <td>&nbsp;</td>
        <td>
          <%= pdf_grey_checkbox(_f, :engine_air_filter) %>
          <p>Engine Air Filter </p>
        </td>
        <td class="centertd">
          <%= pdf_checkbox(_f, :engine_air_filter_serviced) %>
        </td>
      </tr>
      <tr>
        <td>
          <%= pdf_grey_checkbox(_f, :oil_changes) %>
          <p>Oil change & filter</p>
        </td>
        <td class="centertd">
          <%= pdf_checkbox(_f, :oil_changes_serviced) %>
        </td>
        <td>&nbsp;</td>
        <td>
          <%= pdf_grey_checkbox(_f, :engine_coolant_filter) %>
          <p>Engine Coolent</p>
        </td>
        <td class="centertd">
          <%= pdf_checkbox(_f, :engine_coolant_filter_serviced) %>
        </td>
      </tr>
      <tr>
        <td>
          <%= pdf_grey_checkbox(_f, :tire_rotation) %>
