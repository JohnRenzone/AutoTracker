<%= f.fields_for :scheduled_maintenance_item do |_f| %>
    <table width="100%" border="1" cellspacing="0" cellpadding="0">
      <tbody>
      <tr>
        <td colspan="5" class="headCell">SCHEDULED MAINTENANCE ITEMS DUE*</td>
      </tr>
