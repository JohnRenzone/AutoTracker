.inputUnderline {
    border: 0;
    outline: 0;
    border-bottom: 1px solid grey;
    width: auto;
    box-shadow: none;
    -webkit-appearance: none;
    -webkit-border-radius: 0;
    font-weight: 400;

    font-size: 11px;
    padding: 0;
}

.Vehicle_Report_card {
    color: rgb(87, 112, 149);
    text-transform: uppercase;
    text-align: left;
    display: flex;
    align-items: center;
}

input[type=checkbox].css-checkboxGrey {
    position: absolute;
    z-index: -1000;
    left: -1000px;
    overflow: hidden;
    clip: rect(0 0 0 0);
    height: 1px;
    width: 1px;
    margin: -1px;
    padding: 0;
    border: 0;
}
input[type=checkbox].css-checkboxGrey + label.css-labelGrey {
    padding-left: 26px;
    height: 25px;
    display: inline-block;
    line-height: 25px;
    background-repeat: no-repeat;
    background-position: 0 0;
    font-size: 25px;
    vertical-align: middle;
    cursor: pointer;

    font-size:13px;
    margin:3px!important;
}
input[type=checkbox].css-checkboxGrey:checked + label.css-labelGrey {
    background-position: 0 -25px;
}

label.css-labelGrey {
    background-image: asset_data_url('pdf/grey.png');
    -webkit-touch-callout: none;
    -webkit-user-select: none;
    -khtml-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
}

